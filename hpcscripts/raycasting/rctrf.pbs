cd $PBS_O_WORKDIR;   ## This line is needed, do not modify. 

cat batch.txt | while read line
do
	temp=${line##*Data}
	scp -P 8398 $line/${rad}binData.csv hippocampus@cortex.nus.edu.sg:/volume1/Hippocampus/Data$temp/${rad}binData.csv
	if [ $rad -eq 1 ]; then
		scp $line/unityfile_eyelink.csv $line/logs.txt $line/VirtualMazeBatchLog.txt hippocampus@cortex.nus.edu.sg:/volume1/Hippocampus/Data$temp/
	fi
done
